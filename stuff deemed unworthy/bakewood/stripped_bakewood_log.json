{
	"format_version": "1.20.30",
	"minecraft:block": {
		"description": {
			"identifier": "vc:stripped_bakewood_log",
			"menu_category": {
				"category": "nature",
				"group": "itemGroup.name.log"
			},
			"states": {
				"vc:face": [
					0,
					1,
					2,
					3,
					4,
					5,
					6
				]
			}
		},
		"components": {
			"tag:wood": {},
            "tag:minecraft:is_axe_item_destructible": {},
			"minecraft:flammable": {
				"catch_chance_modifier": 5,
				"destroy_chance_modifier": 20
			},
			"minecraft:destructible_by_mining": {
				"seconds_to_destroy": 3
			},
			"minecraft:on_player_placing": {
				"event": "vc:place"
			},
			"minecraft:geometry": "geometry.block",
			"minecraft:material_instances": {
				"up": {
					"texture": "stripped_bakewood_log_top",
					"render_method": "alpha_test"
				},
				"down": {
					"texture": "stripped_bakewood_log_top",
					"render_method": "alpha_test"
				},
				"*": {
					"texture": "stripped_bakewood_log",
					"render_method": "alpha_test"
				}
			}
		},
		"permutations": [
			{
				"condition": "query.block_state('vc:face') == 0",
				"components": {
					"minecraft:transformation": {
						"rotation": [
							0,
							0,
							0
						]
					}
				}
			},
			{
				"condition": "query.block_state('vc:face') == 1",
				"components": {
					"minecraft:transformation": {
						"rotation": [
							90,
							0,
							0
						]
					}
				}
			},
			{
				"condition": "query.block_state('vc:face') == 2",
				"components": {
					"minecraft:transformation": {
						"rotation": [
							0,
							0,
							90
						]
					}
				}
			}
		],
		"events": {
			"vc:place": {
				"set_block_state": {
					"vc:face": "math.floor(query.cardinal_block_face_placed_on/2)"
				}
			}
		}
	}
}