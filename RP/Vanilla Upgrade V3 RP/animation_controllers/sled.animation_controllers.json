{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.sled.general": {
			"states": {
				"default": {
					"animations": [
						"pos"
					],
					"transitions": [
						{
							"slide": "!query.is_on_ground"
						}
					],
					"blend_transition": 0.2
				},
				"slide": {
					"animations": [
						"down"
					],
					"transitions": [
						{
							"default": "query.is_on_ground"
						}
					],
					"blend_transition": 0.2
				}
			}
		},
		"controller.animation.pos.general": {
			"states": {
				"default": {
					"animations": [
						"pos"
					],
					"blend_transition": 0.2
				}
			}
		}
	}
}