{
	"format_version": "1.10.0",
	"animation_controllers": {
		"controller.animation.wolf.angry": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"wolf_angry"
					]
				}
			}
		},
		"controller.animation.wolf.baby_scaling": {
			"initial_state": "default",
			"states": {
				"baby": {
					"animations": [
						"wolf_baby_scaling"
					],
					"transitions": [
						{
							"default": "!query.is_baby"
						}
					]
				},
				"default": {
					"transitions": [
						{
							"baby": "query.is_baby"
						}
					]
				}
			}
		},
		"controller.animation.wolf.head_rot_z": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"rot": "query.is_interested || query.is_shaking_wetness"
						}
					]
				},
				"rot": {
					"animations": [
						"wolf_head_rot_z"
					],
					"transitions": [
						{
							"default": "!query.is_interested && !query.is_shaking_wetness"
						}
					]
				}
			}
		},
		"controller.animation.wolf.look_at": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"wolf_look_at"
					]
				}
			}
		},
		"controller.animation.wolf.setup": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"wolf_setup"
					]
				}
			}
		},
		"controller.animation.wolf.shaking": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"shaking": "query.is_shaking_wetness"
						}
					]
				},
				"shaking": {
					"animations": [
						"wolf_shaking"
					],
					"transitions": [
						{
							"default": "!query.is_shaking_wetness"
						}
					]
				}
			}
		},
		"controller.animation.wolf.digging": {
			"initial_state": "default",
			"states": {
				"default": {
					"transitions": [
						{
							"digging": "query.is_digging"
						}
					]
				},
				"digging": {
					"animations": [
						"wolf_dig"
					],
					"transitions": [
						{
							"default": "!query.is_digging"
						}
					]
				}
			}
		},
		"controller.animation.wolf.sitting": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"wolf_leg_default"
					],
					"transitions": [
						{
							"sitting": "query.is_sitting && !query.property('vc:lying') == true"
						},
						{
							"lying": "query.is_sitting && query.property('vc:lying') == true"
						}
					]
				},
				"sitting": {
					"animations": [
						"wolf_sitting"
					],
					"transitions": [
						{
							"default": "!query.is_sitting"
						},
						{
							"lying": "query.is_sitting && query.property('vc:lying') == true"
						}
					]
				},
				"lying": {
					"animations": [
						"wolf_lying"
					],
					"transitions": [
						{
							"default": "!query.is_sitting"
						},
						{
							"sitting": "query.is_sitting && !query.property('vc:lying') == true"
						}
					]
				}
			}
		},
		"controller.animation.wolf.tail_default": {
			"initial_state": "default",
			"states": {
				"default": {
					"animations": [
						"wolf_tail_default"
					]
				}
			}
		}
	}
}