{
  "covid_wait": {
    "anim_type": "alpha",
    "easing": "linear",
    "duration": 5,
    "from": 1,
    "to": 1,
    "next": "@hud.hide"
  },
  "hide": {
    "anim_type": "size",
    "easing": "linear",
    "duration": 0.01,
    "from": [
      "0%",
      "0%"
    ],
    "to": [
      "0%",
      "0%"
    ]
  },
  "effect_lol": {
    "ignored": "(not ($var_actionbar_text = 'vc.covid_lol'))",
    "type": "image",
    "texture": "textures/ui/effect_lol",
    "size": [
      "8%",
      "8%"
    ],
    "offset": [
      -5,
      -20
    ],
    "anchor_from": "right_middle",
    "anchor_to": "center",
    "layer": 15,
    "anims": [
      "@VU_title.move_in",
      "@hud.covid_wait"
    ]
  },
  "covid_lol": {
    "ignored": "(not ($var_actionbar_text = 'vc.covid_lol'))",
    "type": "image",
    "texture": "textures/ui/covid_effect",
    "size": [
      "7%",
      "7%"
    ],
    "offset": [
      -5,
      -20
    ],
    "anchor_from": "right_middle",
    "anchor_to": "center",
    "layer": 15,
    "anims": [
      "@VU_title.move_in",
      "@VU_title.fadeinout",
      "@hud.covid_wait"
    ]
  },
  "boss_health_panel": {
    "type": "panel",
    "size": [
      182,
      20
    ],
    "anchor_from": "top_middle",
    "anchor_to": "top_middle",
    "$progress_bar_collection": "boss_bars",
    "controls": [
      {
        "vanilla_bossbar": {
          "type": "panel",
          "controls": [
            {
              "boss_name@boss_name_panel": {}
            },
            {
              "progress_bar_for_collections@common.progress_bar_for_collections": {
                "offset": [
                  0,
                  10
                ]
              }
            }
          ],
          "bindings": [
            {
              "binding_name": "#bossName",
              "binding_type": "collection",
              "binding_collection_name": "boss_bars"
            },
            {
              "binding_type": "view",
              "source_property_name": "((#bossName - 'Giant Zombie') = #bossName)",
              "target_property_name": "#visible"
            }
          ]
        }
      },
      {
        "orange_bossbar": {
          "type": "panel",
          "controls": [
            {
              "boss_name@boss_name_panel": {}
            },
            {
              "progress_bar": {
                "type": "panel",
                "anchor_from": "top_middle",
                "anchor_to": "top_middle",
                "offset": [
                  0,
                  10
                ],
                "size": [
                  182,
                  5
                ],
                "controls": [
                  {
                    "empty_progress_bar@common.empty_progress_bar": {
                      "layer": 1
                    }
                  },
                  {
                    "filled_progress_bar@common.filled_progress_bar": {
                      "layer": 2,
                      "clip_pixelperfect": false,
                      "color": [
                        1,
                        0.5,
                        0
                      ],
                      "bindings": [
                        {
                          "binding_name": "#progress_percentage",
                          "binding_name_override": "#clip_ratio",
                          "binding_type": "collection",
                          "binding_collection_name": "$progress_bar_collection"
                        }
                      ]
                    }
                  }
                ]
              }
            }
          ],
          "bindings": [
            {
              "binding_name": "#bossName",
              "binding_type": "collection",
              "binding_collection_name": "boss_bars"
            },
            {
              "binding_type": "view",
              "source_property_name": "(not ((#bossName - 'Giant Zombie') = #bossName))",
              "target_property_name": "#visible"
            }
          ]
        }
      }
    ],
    "bindings": [
      {
        "binding_name": "#bar_visible",
        "binding_type": "collection",
        "binding_collection_name": "boss_bars",
        "binding_name_override": "#visible"
      },
      {
        "binding_name": "#bossName",
        "binding_type": "collection",
        "binding_collection_name": "boss_bars"
      }
    ]
  }
