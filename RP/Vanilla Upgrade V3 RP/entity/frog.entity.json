{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "vc:frog",
      "materials": { "default": "sheep" },
      "textures": {
        "default": "textures/entity/frog"
      },
      "geometry": {
        "default": "geometry.frog"
      },
      "animations": {
        "look_at_target": "animation.common.look_at_target",
        "baby_transform": "animation.sheep.baby_transform",
        "move": "animation.frog.move"
      },
      "scripts": {
        "animate": [
          "look_at_target",
          "move",
          { "baby_transform": "query.is_baby" }
        ]
      },
      "render_controllers": [ "controller.render.zombie" ],
      "spawn_egg": {
        "base_color": "#589c08",
        "overlay_color": "#b6ef74"
      }
    }
  }
}
