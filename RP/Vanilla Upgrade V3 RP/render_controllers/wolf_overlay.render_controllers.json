{
	"format_version": "1.8.0",
	"render_controllers": {
	  "controller.render.wolf_overlay": {
		"arrays": {
		  "textures": {
			"Array.skins": [
			  "Texture.white",
			  "Texture.dark",
			  "Texture.dalmation",
			  "Texture.golden",
			  "Texture.grey"
			]
		  }
		},
		"geometry": "Geometry.default",
		"materials": [ { "*": "Material.default" } ],
		"textures": [
		  "query.is_angry ? Texture.angry : (query.has_owner ? Texture.tame : Texture.default)"
		]
	  }
	}
  }
  