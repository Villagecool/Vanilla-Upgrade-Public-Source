{
  "format_version": "1.16.0",
  "minecraft:entity": {
    "description": {
      "identifier": "vc:illusioner",
      "runtime_identifier": "minecraft:evocation_illager",
      "is_spawnable": true,
      "is_summonable": true,
      "is_experimental": false
    },
    "component_groups": {
      "minecraft:celebrate": {
        "minecraft:behavior.celebrate": {
          "priority": 5,
          "celebration_sound": "celebrate",
          "sound_interval": {
            "range_min": 2.0,
            "range_max": 7.0
          },
          "jump_interval": {
            "range_min": 1.0,
            "range_max": 3.5
          },
          "duration": 30.0,
          "on_celebration_end_event": {
            "event": "minecraft:stop_celebrating",
            "target": "self"
          }
        }
      },
      "minecraft:raid_configuration": {
        "minecraft:dweller": {
          "dwelling_type": "village",
          "dweller_role": "hostile",
          "update_interval_base": 60,
          "update_interval_variant": 40,
          "can_find_poi": false,
          "can_migrate": false,
          "first_founding_reward": 0
        },
        "minecraft:behavior.move_to_village": {
          "priority": 6,
          "speed_multiplier": 0.7
        },
        "minecraft:ambient_sound_interval": {
          "value": 2.0,
          "range": 4.0,
          "event_name": "ambient.in.raid"
        }
      },
      "minecraft:ranged_attack": {
        "minecraft:behavior.ranged_attack": {
          "priority": 4,
          "attack_interval_min": 1.0,
          "attack_interval_max": 1.0,
          "attack_radius": 8.0
        },
        "minecraft:shooter": {
          "def": "minecraft:arrow"
        },
        "minecraft:environment_sensor": {
          "triggers": {
            "filters": {
              "test": "is_underwater",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "event": "minecraft:melee_mode"
          }
        },
        "minecraft:equipment": {
          "table": "loot_tables/entities/illusioner_gear.json"
        }
      },
      "minecraft:melee_attack": {
        "minecraft:behavior.melee_attack": {
          "priority": 4,
          "track_target": true
        },
        "minecraft:attack": {
          "damage": 6
        },
        "minecraft:environment_sensor": {
          "triggers": {
            "filters": {
              "test": "in_water",
              "subject": "self",
              "operator": "==",
              "value": false
            },
            "event": "minecraft:ranged_mode"
          }
        }
      },
      "vc:invis": {
        "minecraft:teleport": {
          "dark_teleport_chance": 0.01,
          "light_teleport_chance": 1,
          "random_teleport_cube": [
            5,
            5,
            5
          ],
          "random_teleports": true,
          "target_distance": 16,
          "max_random_teleport_time": 1,
          "min_random_teleport_time": 0,
          "target_teleport_chance": 1
        },
        "minecraft:timer": {
          "looping": true,
          "time": [
            1,
            2
          ],
          "time_down_event": {
            "event": "vc:uninvis"
          }
        }
      }
    },
    "components": {
      "minecraft:experience_reward": {
        "on_death": "10"
      },
      "minecraft:type_family": {
        "family": [
          "illusioner",
          "orignal",
          "monster",
          "illager",
          "mob"
        ]
      },
      "minecraft:breathable": {
        "total_supply": 15,
        "suffocate_time": 0
      },
      "minecraft:nameable": {},
      "minecraft:loot": {
        "table": "loot_tables/entities/illusioner.json"
      },
      "minecraft:health": {
        "value": 180,
        "max": 190
      },
      "minecraft:hurt_on_condition": {
        "damage_conditions": [
          {
            "filters": {
              "test": "in_lava",
              "subject": "self",
              "operator": "==",
              "value": true
            },
            "cause": "lava",
            "damage_per_tick": 4
          }
        ]
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.9
      },
      "minecraft:movement": {
        "value": 0.5
      },
      "minecraft:navigation.walk": {
        "can_path_over_water": true,
        "can_pass_doors": true,
        "can_open_doors": true,
        "avoid_water": true
      },
      "minecraft:movement.basic": {},
      "minecraft:jump.static": {},
      "minecraft:can_climb": {},
      "minecraft:behavior.float": {
        "priority": 0
      },
      "minecraft:behavior.summon_entity": {
        "priority": 2,
        "summon_choices": [
          {
            "min_activation_range": 3.0,
            "weight": 3,
            "cooldown_time": 5.0,
            "cast_duration": 2.0,
            "particle_color": "#FF664D59",
            "start_sound_event": "cast.spell",
            "sequence": [
              {
                "size": 0.1,
                "base_delay": 0,
                "num_entities_spawned": 1,
                "shape": "circle",
                "entity_lifespan": 0.1,
                "entity_type": "vc:blind<vc:invis>",
                "target": "self"
              },
              {
                "shape": "circle",
                "target": "self",
                "base_delay": 1.0,
                "delay_per_summon": 0.3,
                "num_entities_spawned": 7,
                "entity_type": "vc:illusioner_clone",
                "size": 10,
                "entity_lifespan": 15.5,
                "sound_event": "prepare.attack"
              }
            ]
          },
          {
            "min_activation_range": 3.0,
            "weight": 1,
            "cooldown_time": 0.3,
            "cast_duration": 2.0,
            "particle_color": "#FF664D59",
            "start_sound_event": "prepare.summon",
            "sequence": [
              {
                "shape": "circle",
                "target": "self",
                "base_delay": 1.0,
                "delay_per_summon": 0.05,
                "num_entities_spawned": 1,
                "entity_type": "vc:blind",
                "size": 1,
                "entity_lifespan": 0.5
              }
            ]
          }
        ]
      },
      "minecraft:behavior.send_event": {
        "priority": 3,
        "event_choices": [
          {
            "min_activation_range": 0.0,
            "max_activation_range": 16.0,
            "cooldown_time": 5.0,
            "cast_duration": 3.0,
            "particle_color": "#c000c3",
            "weight": 3,
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "sheep"
                },
                {
                  "test": "is_color",
                  "subject": "other",
                  "value": "red"
                }
              ]
            },
            "start_sound_event": "cast.blind",
            "sequence": [
              {
                "base_delay": 2.0,
                "event": "wololo2",
                "sound_event": "prepare.wololo"
              }
            ]
          }
        ]
      },
      "minecraft:behavior.avoid_mob_type": {
        "priority": 5,
        "entity_types": [
          {
            "filters": {
              "test": "is_family",
              "subject": "other",
              "value": "player"
            },
            "max_dist": 8,
            "walk_speed_multiplier": 0.6,
            "sprint_speed_multiplier": 1.0
          }
        ]
      },
      "minecraft:behavior.random_stroll": {
        "priority": 8,
        "speed_multiplier": 0.6
      },
      "minecraft:behavior.nearest_attackable_target": {
        "priority": 2,
        "entity_types": [
          {
            "filters": {
              "any_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "player"
                },
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "snowgolem"
                },
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "irongolem"
                },
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "wandering_trader"
                }
              ]
            },
            "max_dist": 20
          },
          {
            "filters": {
              "all_of": [
                {
                  "test": "is_family",
                  "subject": "other",
                  "value": "villager"
                },
                {
                  "test": "has_component",
                  "subject": "other",
                  "operator": "!=",
                  "value": "minecraft:is_baby"
                }
              ]
            },
            "max_dist": 20
          }
        ],
        "must_see": true
      },
      "minecraft:behavior.look_at_player": {
        "priority": 9,
        "look_distance": 3.0,
        "probability": 1.0
      },
      "minecraft:behavior.look_at_entity": {
        "priority": 10,
        "look_distance": 8.0,
        "filters": {
          "test": "is_family",
          "subject": "other",
          "value": "mob"
        }
      },
      "minecraft:behavior.hurt_by_target": {
        "priority": 1
      },
      "minecraft:persistent": {},
      "minecraft:physics": {},
      "minecraft:damage_sensor": {
        "triggers": [
          {
            "on_damage": {
              "filters": {
                "test": "is_family",
                "subject": "other",
                "value": "illusioner"
              }
            },
            "deals_damage": false
          }
        ]
      },
      "minecraft:pushable": {
        "is_pushable": true,
        "is_pushable_by_piston": true
      },
      "minecraft:follow_range": {
        "value": 64
      }
    },
    "events": {
      "minecraft:spawn_for_raid": {
        "add": {
          "component_groups": [
            "minecraft:raid_configuration"
          ]
        }
      },
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "minecraft:ranged_attack"
          ]
        }
      },
      "minecraft:start_celebrating": { // Event must be called exactly this for village to fire it when a raid is lost
        "add": {
          "component_groups": [
            "minecraft:celebrate"
          ]
        },
        "queue_command": {
          "command": [
            "playsound mob.illusioner.mirror_move @a ~~~",
            "summon vc:illusioner_clone ^^^-1 ~~ minecraft:start_celebrating",
            "summon vc:illusioner_clone ^^^-2 ~~ minecraft:start_celebrating",
            "summon vc:illusioner_clone ^-2^^ ~~ minecraft:start_celebrating",
            "summon vc:illusioner_clone ^-2^^-1 ~~ minecraft:start_celebrating",
            "summon vc:illusioner_clone ^-2^^-2 ~~ minecraft:start_celebrating"
          ]
        }
      },
      "vc:invis": {
        "add": {
          "component_groups": [
            "vc:invis"
          ]
        },
        "queue_command": {
          "command": [
            "effect @s invisibility 5 1 true",
            "particle vc:poof ~~~",
            "playsound random.poof @a[r=15] ~~~"
          ]
        }
      },
      "vc:uninvis": {
        "remove": {
          "component_groups": [
            "vc:invis"
          ]
        },
        "queue_command": {
          "command": [
            "effect @s invisibility 0 0 true",
            "particle vc:clone_spawn ~~~",
            "particle vc:clone_spawn_box ~~~",
            "particle vc:clone_spawn_pyramid ~~~",
            "playsound mob.illusioner.idle @a[r=15] ~~~"
          ]
        }
      },
      "minecraft:stop_celebrating": {
        "remove": {
          "component_groups": [
            "minecraft:celebrate"
          ]
        }
      },
      "minecraft:melee_mode": {
        "remove": {
          "component_groups": [
            "minecraft:ranged_attack"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:melee_attack"
          ]
        }
      },
      "minecraft:ranged_mode": {
        "remove": {
          "component_groups": [
            "minecraft:melee_attack"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:ranged_attack"
          ]
        }
      }
    }
  }
}