{
	"format_version": "1.21.50",
	"use_beta_features": false,
	"minecraft:block": {
		"description": {
			"menu_category": {
				"category": "nature",
				"group": "itemGroup.name.leaves"
			},
			"identifier": "vc:silver_birch_leaves",
			"states": {
				"vc:connection_north": [false,true],
				"vc:connection_east": [false,true],
				"vc:connection_south": [false,true],
				"vc:connection_west": [false,true]
			}
			//"states": {
			//	"vc:opaque": [
			//		false,
			//		true
			//	]
			//}
		},
		"components": {
			"tag:leaves": {},
			"tag:plant": {},
			"tag:minecraft:is_shears_item_destructible": {},
			"tag:minecraft:is_hoe_item_destructible": {},
			"minecraft:flammable": {
				"catch_chance_modifier": 5,
				"destroy_chance_modifier": 5
			},
			"minecraft:destructible_by_mining": {
				"seconds_to_destroy": 1.1
			},
			"minecraft:light_dampening": 15,
			//"minecraft:custom_components": [
			//	"vc:opaques"
			//],
			"minecraft:liquid_detection": {
				"detection_rules": [{
					"can_contain_liquid": true,
					"liquid_type": "water",
					"stops_liquid_flowing_from_direction": ["north","east","south","west","up","down"],
					"on_liquid_touches": "blocking"
				}]
			},
			"minecraft:geometry": "geometry.no_see_block",
			"minecraft:material_instances": {
				"*": {
					"texture": "vc_silver_birch_leaves",
					"render_method": "blend",
					"ambient_occlusion": true,
					"face_dimming": true
				}
			}
		},
		"permutations": [
			{
				"condition": "query.block_state('vc:connection_north') && query.block_state('vc:connection_east') && query.block_state('vc:connection_south') && query.block_state('vc:connection_west')",
				"components": {
					"minecraft:geometry": "minecraft:geometry.full_block",
					"minecraft:material_instances": {
						"*": {
							"texture": "vc_silver_birch_leaves_opaque",
							"render_method": "opaque"
						}
					}
				}
			}
		]
	}
}