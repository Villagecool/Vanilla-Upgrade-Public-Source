          ]
        }
      },
      "vc:eats": {
        "add": {
          "component_groups": [
            "vc:eating_block"
          ]
        }
      },
      "stop_eating_animation": {
        "remove": {
          "component_groups": [
            "vc:eating_block"
          ]
        }
      },
      "minecraft:ageable_grow_up": {
        "remove": {
          "component_groups": [
            "minecraft:termite_baby"
          ]
        },
        "add": {
          "component_groups": [
            "minecraft:termite_adult"
          ]
        }
