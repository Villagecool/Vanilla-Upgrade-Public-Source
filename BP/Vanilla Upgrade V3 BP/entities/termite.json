      "minecraft:nameable": {
        "default_trigger": {
          "event": "nottoymite",
          "target": "self"
        },
        "name_actions": [
          {
            "name_filter": "toymite",
            "on_named": {
              "event": "toymite",
              "target": "self"
            }
          }
