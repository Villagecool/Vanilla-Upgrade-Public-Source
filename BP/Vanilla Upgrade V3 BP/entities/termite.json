              ]
            },
            "randomize": [
              {
                "weight": 95,
                "remove": {},
                "trigger": "spawn_adult"
              },
              {
                "weight": 5,
                "remove": {},
                "trigger": "spawn_baby"
              }
            ]
          }
        ]
      },
      "eat_block": {
        "queue_command": {
          "command": [
            "scriptevent vc:eat_block"
          ]
        },
        "add": {
          "component_groups": [
            "find_nest"
          ]
        },
        "remove": {
          "component_groups": [
            "vc:eating_block"
          ]
        }
      },
      "eat_block_toymite": {
        "queue_command": {
          "command": [
            "scriptevent vc:eat_block"
          ]
        }
      },
      "playsound": {
        "queue_command": {
          "command": [
            "playsound block.beehive.enter @a ~~~"
          ]
        }
      },
      "spawn_adult": {
        "add": {
          "component_groups": [
            "minecraft:termite_adult",
            "find_food"
          ]
        }
      },
      "find_nest": {
        "remove": {
          "component_groups": [
            "find_food"
          ]
        },
        "add": {
          "component_groups": [
            "find_nest"
          ]
        }
      },
      "find_food": {
        "remove": {
          "component_groups": [
            "find_nest"
          ]
        },
        "add": {
          "component_groups": [
            "find_food"
          ]
        }
