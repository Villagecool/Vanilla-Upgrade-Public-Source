          "priority": 1,
          "target_needed": false,
          "within_radius": 10,
          "start_delay": 0
        }
      },
      "vc:in_nest": {
        "minecraft:physics": {
          "has_collision": false,
          "has_gravity": false
        },
        "minecraft:timer": {
          "looping": false,
          "time": 0.1,
          "time_down_event": {
            "event": "playsound"
          }
        },
        "minecraft:scale": {
          "value": 0
        }
      },
      "vc:out_nest": {
        "minecraft:physics": {
          "has_collision": true,
          "has_gravity": true
        }
      },
      "vc:eating_block": {
        "minecraft:timer": {
          "looping": true,
          "time": 3,
          "time_down_event": {
            "event": "stop_eating_animation"
          }
        },
        "minecraft:is_saddled": {}
      },
      "minecraft:termite_baby": {
        "minecraft:is_baby": {},
        "minecraft:scale": {
          "value": 0.5
        },
        "minecraft:ageable": {
          "duration": 1200,
          "feed_items": "stick",
          "grow_up": {
            "event": "minecraft:ageable_grow_up",
            "target": "self"
          }
        },
        "minecraft:behavior.follow_parent": {
          "priority": 6,
          "speed_multiplier": 1.1
        }
      },
      "minecraft:termite_adult": {
        "minecraft:loot": {
          "table": "loot_tables/entities/termite.json"
        },
        "minecraft:experience_reward": {
          "on_bred": "Math.Random(1,7)",
          "on_death": "query.last_hit_by_player ? Math.Random(1,3) : 0"
        },
        "minecraft:behavior.breed": {
          "priority": 3,
          "speed_multiplier": 1.0
        },
        "minecraft:breedable": {
          "require_tame": false,
          "breeds_with": {
            "mate_type": "vc:termite",
            "baby_type": "vc:termite"
          },
          "breed_items": "stick"
        }
      }
