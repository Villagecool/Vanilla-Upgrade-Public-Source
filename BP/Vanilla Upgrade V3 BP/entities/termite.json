          "on_stay_completed": [
            {
              "event": "eat_block_toymite",
              "target": "self"
            }
          ]
        }
      },
      "find_food": {
        "minecraft:behavior.move_to_block": {
          "priority": 3,
          "search_range": 16,
          "search_height": 10,
          "tick_interval": 1,
