        "remove": {
          "component_groups": [
            "find_nest",
            "find_food"
          ]
        }
      },
      "vc:in_nest": {
        "add": {
          "component_groups": [
            "vc:in_nest",
            "find_food"
          ]
        },
        "remove": {
          "component_groups": [
            "vc:out_nest",
            "find_nest"
          ]
        }
      },
      "vc:out_nest": {
        "remove": {
          "component_groups": [
            "vc:in_nest"
          ]
        },
        "add": {
          "component_groups": [
            "vc:out_nest"
          ]
        }
      },
      "nottoymite": {
        "remove": {
          "component_groups": [
            "toymite"
          ]
        }
      },
      "spawn_baby": {
        "add": {
          "component_groups": [
            "minecraft:termite_baby"
          ]
        }
      },
      "minecraft:entity_born": {
