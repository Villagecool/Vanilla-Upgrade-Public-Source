          "on_reach": [
            {
              "event": "vc:eats",
              "target": "self"
            }
          ],
          "on_stay_completed": [
            {
              "event": "eat_block",
              "target": "self"
            }
          ]
        }
      },
      "find_nest": {
        "minecraft:behavior.find_mount": {
          "avoid_water": false,
          "max_failed_attempts": 5,
