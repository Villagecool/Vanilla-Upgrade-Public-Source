            "oak_log",
            "acacia_log",
            "spruce_log",
            "mangrove_log",
            "cherry_log",
            "dark_oak_log",
            "birch_log",
            "oak_planks",
            "acacia_planks",
            "spruce_planks",
            "mangrove_planks",
            "cherry_planks",
            "dark_oak_planks",
            "birch_planks",
            "vc:palm_log",
            "vc:palm_planks",
            "vc:azalea_log",
            "vc:azalea_planks"
